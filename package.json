{
  "publisher": "merlinalex",
  "name": "what-is-node-version",
  "displayName": "What Is Node Version?",
  "version": "1.0.1",
  "packageManager": "pnpm@9.5.0",
  "description": "",
  "author": "Merlin Alex <merlinalex@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/merlinalex1028/what-is-node-version#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/merlinalex1028/what-is-node-version"
  },
  "bugs": {
    "url": "https://github.com/merlinalex1028/what-is-node-version/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.91.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "What Is Node Version extension configuration",
      "properties": {
        "what-is-node-version.useVersion": {
          "type": "string",
          "default": "",
          "description": "The node version you project use."
        }
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.22.0",
    "@antfu/ni": "^0.21.12",
    "@types/node": "^20.14.10",
    "@types/vscode": "^1.91.0",
    "@vscode/vsce": "^2.29.0",
    "bumpp": "^9.4.1",
    "eslint": "^9.6.0",
    "esno": "^4.7.0",
    "pnpm": "^9.5.0",
    "rimraf": "^5.0.9",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vitest": "^1.6.0"
  }
}
